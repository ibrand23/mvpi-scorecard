{
  "name": "mvpi-scorecard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "prebuild": "node scripts/set-build-number.js",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "auto-commit": "node auto-commit.js",
    "dev:watch": "concurrently \"npm run dev\" \"npm run auto-commit\"",
    "commit:manual": "git add . && git commit -m \"Manual commit: $(date)\" && git push origin main"
  },
  "dependencies": {
    "@fontsource/overpass": "^5.2.8",
    "next": "15.5.3",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.2.1",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
